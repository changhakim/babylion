<h1>new Post</h1>
<form action="/posts/create" method="post" enctype="multipart/form-data">
  <!-- csrf token 추가 -->
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %> 
  <input type="hidden" name="input_user" value="<%= current_user.id %>">
  <p>title : <input type="text" name="input_title"></p>
  <div>content : <textarea name="input_content" cols="30" rows="10"></textarea></div>
  <input type="submit" value="save">
</form>
